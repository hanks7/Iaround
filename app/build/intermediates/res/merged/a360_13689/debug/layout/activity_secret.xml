<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x36"
        android:layout_marginTop="@dimen/y38"
        android:layout_marginBottom="@dimen/y18"
        android:text="@string/userinfo_secret_data"
        android:textColor="#9B9B9B"
        android:textSize="@dimen/y36" />

    <LinearLayout
        android:id="@+id/ll_user_secret_income"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_income"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_user_income"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="100W"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:layout_marginLeft="@dimen/x36" />

    <LinearLayout
        android:id="@+id/ll_user_secret_own_house"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_own_house"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_own_house"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:layout_marginLeft="@dimen/x36" />

    <LinearLayout
        android:id="@+id/ll_user_secret_own_car"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_own_car"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_own_car"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:layout_marginLeft="@dimen/x36" />

    <LinearLayout
        android:id="@+id/ll_user_secret_school"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_university"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_user_university"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:layout_marginLeft="@dimen/x36" />
    <LinearLayout
        android:id="@+id/ll_user_secret_company"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_company"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_user_company"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:layout_marginLeft="@dimen/x36" />

    <LinearLayout
        android:id="@+id/ll_user_secret_occupation"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y123"
        android:background="@color/main_bg_color"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="@dimen/x376"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x36"
            android:text="@string/userinfo_secret_occupation"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/y42" />

        <TextView
            android:id="@+id/tv_user_occupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#222222"
            android:textSize="@dimen/y42" />
    </LinearLayout>



</LinearLayout>
